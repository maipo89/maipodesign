#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/jshint@2.13.6/node_modules/jshint/bin/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/jshint@2.13.6/node_modules/jshint/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/jshint@2.13.6/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/node_modules:/Applications/MAMP/htdocs/wordpress/node_modules:/Applications/MAMP/htdocs/node_modules:/Applications/MAMP/node_modules:/Applications/node_modules:/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/jshint/bin/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/jshint/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/jshint@2.13.6/node_modules/jshint/bin/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/jshint@2.13.6/node_modules/jshint/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/jshint@2.13.6/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/node_modules:/Applications/MAMP/htdocs/wordpress/node_modules:/Applications/MAMP/htdocs/node_modules:/Applications/MAMP/node_modules:/Applications/node_modules:/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/jshint/bin/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/jshint/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jshint/bin/jshint" "$@"
else
  exec node  "$basedir/../jshint/bin/jshint" "$@"
fi
