#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/webpack@5.91.0_webpack-cli@4.10.0/node_modules/webpack/bin/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/webpack@5.91.0_webpack-cli@4.10.0/node_modules/webpack/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/webpack@5.91.0_webpack-cli@4.10.0/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/node_modules:/Applications/MAMP/htdocs/wordpress/node_modules:/Applications/MAMP/htdocs/node_modules:/Applications/MAMP/node_modules:/Applications/node_modules:/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/webpack/bin/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/webpack/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/webpack@5.91.0_webpack-cli@4.10.0/node_modules/webpack/bin/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/webpack@5.91.0_webpack-cli@4.10.0/node_modules/webpack/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/webpack@5.91.0_webpack-cli@4.10.0/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/.pnpm/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/node_modules:/Applications/MAMP/htdocs/wordpress/node_modules:/Applications/MAMP/htdocs/node_modules:/Applications/MAMP/node_modules:/Applications/node_modules:/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/webpack/bin/node_modules:/Applications/MAMP/htdocs/wordpress/wp-content/themes/boilerplate/node_modules/webpack/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../webpack/bin/webpack.js" "$@"
fi
